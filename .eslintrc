{  
  "parser": "babel-eslint",
    "rules": {      
      "indent": ["error", 2],      
      "no-unused-vars": 0,
      "no-use-before-define": 0,      
      "arrow-parens": 0,      
      "no-restricted-globals": 0    
  },
  "extends": ["airbnb-base"],
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "document": false,    
      "L": true,
      "onMapClick": true
  }  
}
